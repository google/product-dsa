  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug install script (Python)",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/install/cloud_env_setup.py",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": false,
      "args": [
        "--config", "config.yaml"
        //"--client-secrets-file", "client_secrets.json"
      ],
      "env": {
        "PYTHONPATH": "."
      }
    }, {
      "name": "Debug main script (Python)",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/app/main.py",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": false,
      "args": [
        "--config", "config.yaml"
        //"--client-secrets-file", "client_secrets.json"
      ],
      "env": {
        "PYTHONPATH": "."
      }
    }, {
      "name": "Debug WebApp (Python)",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/server/server.py",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": false,
      "args": [
        "--config", "config.yaml"
        //"--client-secrets-file", "client_secrets.json"
      ],
      "env": {
        "PYTHONPATH": "."
      }
    }
  ]
}